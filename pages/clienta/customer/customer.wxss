/* pages/apipageone/apipageone.wxss */
/*顶部搜索框样式*/
page{background-color: #fff;}
.content{
  position:fixed;
  bottom: 100rpx;
  width: 100%;
}
.search-box{
  background-color: #fff;
  padding:8rpx 16rpx;
  padding-right:0; 
  display: flex;
  align-items: center;
  
}
.input-box{
  display: flex;
  align-items: center;
  border-radius: 10rpx;
  background-color: #efeff4;
  width: 635rpx;
  box-sizing: border-box;
  z-index: 999
}
.search icon{
  margin:0 22rpx;
}
.search input{
  font-size: 26rpx;
  width: 575rpx;
  height: 68rpx;
  line-height:68rpx;
  box-sizing: border-box;
}
.label-box{
  position: absolute;
  right:0;
  top:10rpx;
  text-align: center;
  box-sizing: border-box;
  width: 100rpx;
  font-size:20rpx; 
  height: 68rpx;
  line-height: 115rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center 0;
  background-size: 38%;
}
.label-box-cancel{
  position: absolute;
  right:0;
  top:10rpx;
  text-align: center;
  width: 100rpx;
  font-size:30rpx; 
  color: #ffa134;
  height: 68rpx;
  line-height: 68rpx;
  background: #fff;
  z-index: 990;
}
/*tab分页*/
.tab-box{
  height:80rpx;
  width: 100%;
  margin-top:10rpx;
  margin-bottom: 15rpx;
  box-sizing:border-box;
}
.tab-box-title{
  width: 100%;
  height:75rpx;
  line-height: 88rpx;
  display: flex;
  justify-content: center;
}
.tab-box-title-l{
  box-sizing: border-box;
  padding-right:65rpx !important;
  width: 375rpx;
  display: flex;
  justify-content: flex-end;
  font-size: 34rpx;
  color: rgb(53, 53, 53);
}
.tab-box-title-r{
  width: 375rpx;
  padding-left: 65rpx !important;
  display: flex;
  justify-content: flex-start;
  font-size: 34rpx;
  color: rgb(53, 53, 53);
}
.tab-box-title-r-true{
  color:#ffa134 !important;
}
.tab-box-solid{
  margin:0 auto;
  height:4rpx;
  width: 135rpx;
  position: relative;
  left:-130rpx;
  background-image:linear-gradient( 0deg, rgb(255,186,23) 0%, rgb(255,160,25) 100%);
  transition: all 0.3s ease;
}
.tab-box-solid-false{
  margin:0 auto;
  height:4rpx;
  width: 135rpx;
  position: relative;
  left:130rpx;
  background-image:linear-gradient( 0deg, rgb(255,186,23) 0%, rgb(255,160,25) 100%);
  transition: all 0.3s ease;
}
/*用户index*/
.portrait-box{
  margin-left: 0;
  display:inline-block;
  height:74rpx;
  width:74rpx;
  min-height:74rpx;
  min-width:74rpx;
  margin:18rpx;
  border-radius:50%;
  border:1rpx solid #e5e5e5;
}
/*搜索弹出层样式*/
.soulist-box{
  position: fixed;
  width: 100%;
  bottom: 100rpx;
  z-index: 990;
  border-top: 1px solid #efeff4;
  box-shadow:0 10rpx 10rpx 0rpx #efeff4 inset;
  background:#fff;
  padding-left:20rpx ;
  box-sizing: border-box;
}
.i-index-demo-item{
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  position: relative;
  border-bottom:1rpx solid #e5e5e5;
}
.soulist-item-box{
  height:100rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.red-dian{
  border-radius: 50%;
  background-color: rgb(255, 95, 25);
  width: 14rpx;
  max-width: 14rpx;
  height: 14rpx;
  margin-left: 8rpx;
  position: relative;
  top:44rpx;
  transform: translateY(-50%);
}
.red-dian-it{
  border-radius: 50%;
  background-color: rgb(255, 95, 25);
  width: 14rpx;
  max-width: 14rpx;
  height: 14rpx;
  position: absolute;
  top:10rpx;
  left:10rpx;
}
.soutrue-box{
  min-width: 710rpx;
  box-sizing: border-box;
  text-align: center;
  position: absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  font-size: 26rpx;
  color: #000;
}
.chat-none-box{
  margin-top:1rpx;
  font-size: 32rpx;
  color: rgb(153, 153, 153);
  background-color: #f0f1f6;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.chat-none-img{
  position: absolute;
  top:50%;
  transform: translateY(-50%);
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  font-size: 28rpx;
  color:#717173;
  text-align: center;
}
.chat-none-text{
  margin-top:35rpx;
  padding:22rpx 46rpx !important;
  line-height: 1;
  box-sizing: border-box;
  border:1rpx solid #666;
  font-size: 28rpx;
  color:#3b3b3b;
}
.i-index-item-header-stock{
  height:44rpx;
  line-height:44rpx;
  background:#f0f1f6;
  font-size:28rpx;
  padding-left:10px;
  width:100%;
  box-sizing:border-box;
  color:black;
}
.i-index-item-header-latent{
  height:44rpx;
  line-height:44rpx;
  background:#f0f1f6;
  font-size:28rpx;
  padding-left:10px;
  width:100%;
  box-sizing:border-box;
  color:black;
}
.i-index-fixed{
  position: fixed;
  right:0;
  top:60%;
  z-index:10;
  padding:0 10rpx;
  transform:translateY(-50%);
}
.i-index-fixed-item{
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  color: rgb(102, 102, 102);
  font-size:20rpx;
  border-radius:50%;
  box-sizing: border-box;
}
.i-index-fixed-item-true{
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  background-color: rgb(255, 138, 1);
  box-sizing: border-box;
  color:#fff;
  font-size:20rpx;
  border-radius:50%
}
  