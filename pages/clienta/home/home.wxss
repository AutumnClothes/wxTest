/* home/home.wxss */
.top{
  overflow: hidden;
  height: 407rpx;
  width: 100%;
  background-repeat: no-repeat;
  background-position: center;
  background-size: 100% auto;
  position: absolute;
}
.group-box{
  position: fixed;
  box-sizing: border-box;
  /* width:170rpx; */
  padding:0 30rpx !important;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 32rpx;
  z-index:998;
  font-size: 32rpx;
  color: #fff;
  background-color: #cacaca
}
.group-img{
  background-image: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100%;
  width:32rpx;
  height:32rpx;
  margin-right:10rpx;
}
.home-top-box{
  width:100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  z-index: 998;
}
.home-title-box{
  width:546rpx;
  padding-left:75rpx;
  font-size: 44rpx;
  color:#fff;
  line-height: 1;
}
.home-title-top{
  font-size: 44rpx;
  font-weight: 600;
  margin-bottom:10rpx;
}
.home-mycard{
  width:204rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  /* padding:18rpx 0 !important; */
  line-height: normal;
  background-color: #fff;
  border-radius: 0;
  border-top-left-radius: 24rpx;
  border-bottom-left-radius: 24rpx;
}
.home-mycard-portrait{
  width:80rpx;
  height:80rpx;
  min-width:80rpx;
  min-height:80rpx;
  border-radius: 50%;
  background-color: #ccc;
  margin:18rpx;
}
.home-mycard-text{
  color:rgb(51, 51, 51);
  font-size: 32rpx;
}
.content-item{
  min-width: 750rpx;
  display:flex;
  background-color: #fff;
  box-sizing: border-box;
  padding: 30rpx 28rpx;
  position: relative;
  transition: all 0.2s linear;
  height:200rpx;
  border-top:1px solid #f0f1f6
}
.content-item-img{
  width: 140rpx;
  height: 140rpx;
}
.content-item-ite{
  margin-left: 23rpx;
  width: 530rpx;
}
.content-item-text{
  font-size: 32rpx;
  color: rgb(51, 51, 51);
  height: 85rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 20rpx;
}
.content-item-time{
  font-size: 20rpx;
  color: rgb(171, 171, 171);
}
.home-newmsg-box{
}
.hone-func-box{
  display: flex;
  justify-content: center;
  align-items: center;
  height:150rpx;
  width: 694rpx;
  background-color: #fff;
  box-shadow: 0rpx 5rpx 40rpx 0rpx rgba(179, 179, 179, 0.33);
  position: relative;
  /* top:332rpx; */
  left:28rpx;
  z-index: 998;
  border-radius: 14rpx;
}
.hone-func-ite{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 28rpx;
  color:#000;
  width:350rpx;
  height:150rpx;
  line-height: normal;
}
.hone-func-itee-top{
  font-size:38rpx;font-weight: 500;
}
.label-box{
  box-sizing: border-box;
  height: 88rpx;
  padding:40rpx 28rpx 20rpx 28rpx;
  width: 100%;
  margin-top: 1rpx;
  background-color: #fff;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.label-itm{
  float: left;
  box-sizing: border-box;
  height:56rpx;
  line-height: 56rpx;
  text-align: center;
  border-radius: 8rpx;
  margin-right:23rpx;
  font-size: 28rpx;
  background-color: #f5f5f5
}
.public-button_one{
  background-color: rgb(25, 25, 25);
  color:#cfae82 !important;
  width:auto;
  padding:0 50rpx !important;
  min-width:380rpx;
}
.sticky-top{
  width:100%;
  /* background-color: #40a4ff; */
  background-color: #ededed;
  z-index:999;
  position: fixed;
  top:0;
}
.tabTitleList-box{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width:100%;
  background-color: #fff;
}
.tabTitleList-box-false{
  position: fixed;
  z-index: 999;
  border-bottom:1px solid #f0f1f6;
  box-sizing: border-box;
}
.tabTitleList-box-ite{
  width:107rpx;
  height:72rpx;
  display: flex;
  justify-content: center;
  align-items: flex-end;
  padding-bottom: 20rpx !important;
  line-height: 1;
  font-size: 32rpx;
  color:#80848f;
  margin:0 !important;
}
.tabTitleList-box-ite-true{
  font-weight: 500;
  font-size: 34rpx !important;
  color:rgb(51, 51, 51) !important;
}
.tabTitleList-solid{
  height:6rpx;
  width:65rpx;
  position:absolute;
  left:21rpx;
  /* background-image:linear-gradient( 0deg, rgb(255,186,23) 0%, rgb(255,160,25) 100%); */
  background-color: rgb(51, 51, 51);
  transition:all 0.3s ease;
  bottom:-1rpx;
  z-index:999;
}