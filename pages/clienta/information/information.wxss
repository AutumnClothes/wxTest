.content{
  background-color: #f0f1f6;
  position:fixed;
  bottom: 100rpx;
  width: 100%;
}
/*顶部tap样式*/
.group-box{
  position: fixed;
  box-sizing: border-box;
  width:170rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 37rpx;
  z-index:998;
}
.group-img{
  width: 33rpx;
  height: 28rpx;
  margin-right: 10rpx;
}
.group-text{
  font-size: 32rpx;
  color: #000;
}
/*tab分页*/
.tab-box{
  height:80rpx;
  width: 100%;
  padding-bottom:12rpx; 
  background-color: #fff;
}
.tab-box-title{
  width: 100%;
  height:75rpx;
  display: flex;
  align-items: flex-end;
}
.tab-box-title-l{
  width: 50%;
  height:75rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding-right: 92rpx;
  box-sizing: border-box;
  font-size: 34rpx;
  color: rgb(53, 53, 53);
}
.tab-box-title-r{
  width: 50%;
  height:75rpx;
  padding-left: 92rpx;
  display: flex;
  align-items:center;
  box-sizing: border-box;
  font-size: 34rpx;
  color: rgb(53, 53, 53);
}
.tab-box-title-r-true{
  color:#ffa134 !important;
}
.tab-box-solid{
  margin:0 auto;
  height:4rpx;
  width: 68rpx;
  position: relative;
  left:-126rpx;
  background-image:linear-gradient( 0deg, rgb(255,186,23) 0%, rgb(255,160,25) 100%);
  transition: all 0.3s ease;
}
.tab-box-solid-false{
  margin:0 auto;
  height:4rpx;
  width: 70rpx;
  position: relative;
  left:126rpx; 
  background-image:linear-gradient( 0deg, rgb(255,186,23) 0%, rgb(255,160,25) 100%);
  transition: all 0.3s ease;
}
/*聊天.css*/
.notice{
  display:flex;
  justify-content:center;
  align-items:center;
  box-sizing: border-box;
  height: 136rpx;
  line-height: 136rpx;
  border-radius: 10rpx;
  margin: 20rpx 25rpx 21rpx 25rpx;
  background-color: #fff;
  font-size: 32rpx;
  color: #ff8a01;
}
.notice-text{
  box-sizing: border-box;
  width: 490rpx;
  padding-left:21rpx;
}
.notice-time{
  color: #dfdfdf
}
.chat-none-box{
  font-size: 32rpx;
  color: rgb(153, 153, 153);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.chat-none-img{
  position: absolute;
  top:50%;
  transform: translateY(-100%);
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  font-size: 28rpx;
  color:#717173;
}
.chat-none-text{
  position: absolute;
  top:50%;
  margin-top:35rpx;
  padding:22rpx 46rpx !important;
  line-height: 1;
  box-sizing: border-box;
  /* border:1rpx solid #666; */
  font-size: 28rpx;
  color:#cfae82 !important;
  background-color: rgb(25, 25, 25);
}
.bell-box{
  width: 101rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: rgb(255, 240, 201);
  background-repeat: no-repeat;
  background-size: 54%;
  background-position: center;
  background-image: url(data:image/png;base64,)
}
.linkman-box{
  box-sizing: border-box;
  border-bottom: 1rpx solid #e5e5e5;
  background-color: #fff;
  height:136rpx;
  padding: 17rpx;
  display: flex;
  align-items: center;
}
.linkman-portrait{
  height: 100rpx;
  width: 100rpx;
  min-width: 100rpx;
  min-height: 100rpx;
  box-sizing: border-box;
  border-radius: 50%;
}
.linkman-text{
  width:480rpx;
  padding-left: 20rpx;
  line-height: 45rpx
}
.linkman-text-title{
  color: #000;
  font-size: 34rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.linkman-text-content{
  font-size: 28rpx;
  color: rgb(170, 170, 170);
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  width: 535rpx
}
.linkman-time{
  position: relative;
  top:-28rpx;
  width: 140rpx;
  color:#dfdfdf;
  font-size: 24rpx;
  text-align: right;
}
/*动态.css*/
.label-box{
  box-sizing: border-box;
  /* border: 1rpx solid red; */
  height: 88rpx;
  padding:16rpx;
  width: 100%;
  margin-top: 1rpx;
  background-color: #fff;
}
.label-itm{
  float: left;
  box-sizing: border-box;
  height:56rpx;
  line-height: 56rpx;
  text-align: center;
  width: 160rpx;
  border-radius: 8rpx;
  margin:0 10rpx;
  font-size: 28rpx;
  background-color: #f5f5f5
}
