.dynamic{
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 14rpx;
  height: 136rpx;
  margin: 0 25rpx 20rpx 25rpx;
  padding: 19rpx;
}
.dynamic-portrait-icon{
  transform: translateY(-35rpx);
  height: 36rpx;
  line-height: 36rpx;
  color:#fff;
  padding: 0 12rpx;
  font-size: 24rpx;
  text-align: center;
  border-radius: 18rpx;
  background-color: #ff5f19;
  position: absolute;
  left:80rpx;
}
.dynamic-icon{
  border-radius: 50%;
  background-color: rgb(255, 95, 25);
  width: 14rpx;
  height: 14rpx;
}
.dynamic-icon-tab{
  margin:9rpx;
}
.dynamic-icon-ite{
  position: absolute;
  top:-7rpx;
  left:-7rpx;
}
.dynamic-text{
  width: 480rpx;
  margin-left: 20rpx;
  font-size: 32rpx;
}
.dynamic-text-box{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}
.dynamic-text text{
  float: left;
  color: rgb(170, 170, 170);
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
  max-width:300rpx;
}
.dynamic-text-last{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  max-width: 480rpx !important;
}
.dynamic-time{
  position: absolute;
  top:0rpx;
  right: 0rpx;
  color:#dfdfdf;
  float: right;
  font-size: 24rpx;
  text-align: right;
  height:45rpx;
  line-height: 54rpx;
}
.guidance1-box{
  position: fixed;
  bottom:20rpx;
  left:50%;
  transform: translateX(-50%);
  display: flex;
  justify-content: center;
  align-items: center;
  padding:30rpx 30rpx 30rpx 20rpx;
  background-color: #fff;
  font-size:28rpx;
  z-index: 999;
  border-radius: 14rpx;
}
.guidance-bottom{
  font-size: 26rpx;
  color:#999;
  position: absolute;
  bottom:20rpx;
  right:35rpx;
}
.guidance-box_four{
  position: absolute;
  margin-top:0;
  top:150rpx;
  width:auto;
  left:40rpx;
  right:auto;
  line-height: 1;
  height:88rpx;
  display: flex;
  align-items: center;
  padding:0 35rpx;
}
.guidance-triangle_four{
  top:-26rpx;
}
.guidance-triangle_thr{
  top:-26rpx;
  right:115rpx;
}
.guidance-box_thr{
  position: fixed;
  right:20rpx;
  text-align: center;
}

