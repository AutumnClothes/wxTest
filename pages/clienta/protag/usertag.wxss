/* pages/clienta/usertag/usertag.wxss */
.container{
  position: absolute;
  box-sizing: border-box;
  overflow:hidden;
  top: 0;
  left:0;
  bottom: 0;
  right: 0;
}
.content{
  border-top:1rpx solid #f0f1f6;
}
.bottom-box{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 1;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  border-top: 1rpx solid #f0f1f6;
  box-sizing: border-box;
  color:#333;
}
/*蒙层*/
.modal-input-meng{
  position: fixed;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #000;
  opacity:0.3;
  z-index: 998;
}
.modal-title{
  width: 100%;
  text-align: center;
  margin:27rpx 0 35rpx 0;
  font-size: 34rpx;
  color:#2d2e33;
}
.modal-input-box{
  position: fixed;
  top:40%;
  width: 70%;
  left:50%;
  transform: translate(-50%,-50%);
  border:1rpx solid #ccc;
  background-color: #fff;
  border-radius: 10rpx;
  z-index: 999;
}
.modal-input{
  padding: 20rpx 12rpx;
  margin: 0 45rpx;
  height:76rpx;
  font-size: 32rpx;
  box-sizing: border-box;
  color: #383842;
}
.modal-button-box{
  margin-top: 62rpx;
  display: flex;
  align-items: center;
}
.modal-button{
  width: 50%;
  box-sizing: border-box;
  border-top:1rpx solid #ccc;
  height:86rpx;
  line-height: 86rpx;
  text-align: center;
}
.modal-button-left{
  color: #b0b3ba;
}
.modal-button-right{
  border-left:1rpx solid #ccc;
  color:#ff8a01;
}
/*标签页*/
.usertag-ul{
  box-sizing: border-box;
  position: relative;
  border-bottom:1rpx solid #f0f1f6;
}
.usertag-li-f{
  box-sizing: border-box;
  font-size: 28rpx;
  color: rgb(102, 102, 102);
  width: 100%;
  height:90rpx;
  line-height: 90rpx;
  display: flex;
  padding-left:60rpx;
}
.usertag-li-f-g{
  font-size: 28rpx;
  height:28rpx;
  line-height: 28rpx;
  color: rgb(102, 102, 102);
}
.usertag-li-icon{
  width: 8rpx;
  height:30rpx;
  position: absolute;
  top:30rpx;
  left:30rpx;
}
.usertag-li-l{
  display: inline-block;
  width: 100%;
  padding-left:10rpx;
  clear: none;
  min-height:40rpx;
}
.usertag-li-v {
  font-size: 28rpx;
  float: left;
  padding: 0 22rpx !important;
  margin:8rpx 0 28rpx 10rpx;
  border-radius: 8rpx;
  background-color: rgb(245, 245, 245);
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  color: #515151;
  clear: none;
}
.usertag-ul:last-child{
  border:none;
}
.usertag-li-v-true{
  font-size: 28rpx;
  float: left;
  padding: 0 12rpx;
  margin:8rpx 0 28rpx 10rpx;
  border-radius: 8rpx;
  background-color: #ff8a01;
  height: 52pxrpx;
  line-height: 52rpx;
  text-align: center;
  color: #fefefe;
  clear: none;
}
.usertag-li-l text{
  margin-left:10rpx;
  color: #999
}
.bottom-box{
  background-color: #fff;
}
.bottom-box-one{
  position: fixed;
  z-index: 999;
  background-color: #fff;
}
/*标签管理*/
.icon-left-box {
  width: 50%;
  height:100%;
  display: flex;
  align-items: center;
}
.icon-right-box{
  width: 50%;
  height:100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.icon-left{
  width:32rpx;
  height:32rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100%;
  background-position: center;
  position: relative;
  left: 35rpx;
}
.icon-right-img{
  width: 62rpx;
  height:60rpx;
  line-height: 60rpx;
}
.icon-right-sort{
  width:32rpx;
  height:32rpx;
  margin-right: 30rpx;
}
.icon-right-del{
  width:32rpx;
  height:32rpx;
  margin-right: 30rpx;
}
.icon-right-add{
  width:32rpx;
  height:32rpx;
  margin-right: 30rpx;
}
.bottom-box-two view{
  float: left;
  width: 50%;
  clear: none;
  box-sizing: border-box;
}
.bottom-box-two view:first-child{
  border-right:1rpx solid #f0f1f6;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 8%;
  background-position: 20% 52%;
}
.bottom-box-two view:last-child{
  border-left:1rpx solid #f0f1f6;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 8%;
  background-position: 20% 52%;
}
.bottom-box-two text{
  color: #ff8a01
}
/*点击修改后弹出的input*/
.alter-input{
  height: 28rpx;
  line-height: 28rpx;
  font-size: 28rpx;
  padding: 0;
  box-sizing: border-box;
}
/*返回按钮.css*/
.back-box{
  position: fixed;
  float: left;
  width:70rpx;
  height: 37rpx;
  margin-left:20rpx;
  font-size: 30rpx;
  line-height: 37rpx;
}