.self-product-box{
    position: relative;
    top:30rpx;
    width: 640rpx;
    margin: 0 auto;
  }
  .self-column-title{
    width:100%;
    height:100rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
  }
  .self-column-title-l{
    /* padding-left: 30rpx; */
    font-size: 32rpx;
    color: rgb(26, 26, 26);
  }
  .self-column-title-r{
    padding-right: 20rpx;
    font-size: 30rpx;
    color: rgb(153, 153, 153);
  }
  .self-column-title-r image{
    position: relative;
    top:2rpx;
    width: 16rpx;
    height: 25rpx;
    margin-left: 10rpx;
  }
  .product-add{
    width:640rpx;
    height:408rpx;
    border-radius:14px;
    background-color: #f0f1f6;
    box-shadow:0px 5rpx 20rpx 0px rgba(179, 179, 179, 0.33);
    /* margin:0 auto; */
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .exemption-box{
    font-size: 32rpx;
    line-height:52rpx;
    width:646rpx;
    margin:0 auto;
    color:rgb(50, 50, 50);
    text-align: center;
  }